public class Rootkit {
	private static Image SPRITE;
	
	static {
		// initialize the sprite
		try {
			SPRITE = new Image("res/images/enemies/tracking_cookie.png");
		} catch (SlickException e) {
			e.printStackTrace();
		}
	}
}
